<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Item4.Item4"
    android:orientation="vertical">
    <include layout="@layout/head"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/darker_gray"
        android:orientation="vertical"
        android:padding="10dp">
        <TextView
            android:id="@+id/top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="40dp"
            android:textColor="#000"
            android:background="#fff"
            android:gravity="center"
            android:text="4号车间-吉普汽车生产线"
            android:layout_gravity="center"
            android:paddingHorizontal="50dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_margin="50dp">
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:background="#000"
                    android:layout_gravity="center_vertical|bottom"
                    android:layout_marginBottom="25dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical|bottom">
                    <ImageView
                        android:id="@+id/Im1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="40dp"
                        android:background="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im3"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im4"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im5"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im6"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/yuan_w"
                        android:layout_marginLeft="40dp"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical|bottom">
                    <ImageView
                        android:id="@+id/Ia1"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginRight="40dp"
                        android:background="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia2"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia3"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia4"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia5"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia6"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="40dp"
                        android:src="@drawable/exclamation"/>

                </LinearLayout>
            </FrameLayout>
            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:layout_gravity="right">
                <View
                    android:layout_width="3dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    android:layout_gravity="center_horizontal"/>
                <ImageView
                    android:id="@+id/Im7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/yuan_w" />
                <ImageView
                    android:id="@+id/Ia7"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/exclamation" />
            </FrameLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:background="#000"
                    android:layout_gravity="top"
                    android:layout_marginTop="25dp"/>
                <View
                    android:layout_width="3dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    android:layout_marginLeft="23dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical|top">
                    <ImageView
                        android:id="@+id/Im13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="40dp"
                        android:src="@drawable/yuan_w" />
                    <ImageView
                        android:id="@+id/Im12"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im11"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im10"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im9"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im8"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/yuan_w"
                        android:layout_marginLeft="40dp"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical|top">
                    <ImageView
                        android:id="@+id/Ia13"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginRight="40dp"
                        android:src="@drawable/exclamation" />
                    <ImageView
                        android:id="@+id/Ia12"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia11"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia10"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia9"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia8"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:src="@drawable/exclamation"
                        android:layout_marginLeft="40dp"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left">
                <ImageView
                    android:id="@+id/Im14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/yuan_w" />
                <ImageView
                    android:id="@+id/Ia14"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/exclamation" />
            </FrameLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:background="#000"
                    android:layout_gravity="center|bottom"
                    android:layout_marginBottom="25dp"/>
                <View
                    android:layout_width="3dp"
                    android:layout_height="match_parent"
                    android:background="#000"
                    android:layout_marginLeft="23dp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical|bottom">
                    <ImageView
                        android:id="@+id/Im15"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="40dp"
                        android:src="@drawable/yuan_w" />
                    <ImageView
                        android:id="@+id/Im16"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im17"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im18"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im19"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/yuan_w"/>
                    <ImageView
                        android:id="@+id/Im20"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/yuan_w"
                        android:layout_marginLeft="40dp"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical|bottom">
                    <ImageView
                        android:id="@+id/Ia15"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginRight="40dp"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia16"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia17"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia18"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia19"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/exclamation"/>
                    <ImageView
                        android:id="@+id/Ia20"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/exclamation"
                        android:layout_marginLeft="40dp"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>